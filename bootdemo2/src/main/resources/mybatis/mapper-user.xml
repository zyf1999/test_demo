<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jk.dao.UserMapper1">
    <select id="findUserByLoginNumber" parameterType="string" resultType="com.jk.bean.UserBean">
        select * from t_user u where u.login_number=#{loginNumber}
    </select>
</mapper>